{
	global_unrest = -2
	global_autonomy = -0.05
	
	allow = {
		OR = {
			religion = mystic_accord
		}
	}
	
	sprite = 1
	
	ai_will_do = {
		factor = 1
		modifier = {
			factor = 2
			personality = ai_administrator
		}
	}
}
		
